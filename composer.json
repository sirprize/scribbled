{
    "name": "sirprize/scribbled",
    "type": "library",
    "description": "Blog aware note-taking app for scribblers and coders",
    "keywords": ["markdown", "textile", "html5", "responsive", "mobile", "symfony"],
    "homepage": "http://scribbled.io",
    "license": "MIT",
    "authors": [
        { "name": "Christian Hoegl", "email": "chrigu@sirprize.me" }
    ],
    "autoload": {
        "psr-0": { "Sirprize\\Scribbled": "lib/" }
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/sirprize/paginate" },
        { "type": "vcs", "url": "https://github.com/sirprize/scribble" },
        { "type": "vcs", "url": "https://github.com/sirprize/scrubble" }
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/common": "2.2.*",
        "pimple/pimple": "1.0.*",
        "symfony/event-dispatcher": "2.1.*",
        "symfony/http-foundation": "2.1.*",
        "symfony/http-kernel": "2.1.*",
        "symfony/routing": "2.1.*",
        "symfony/templating": "2.1.*",
        "sirprize/scribble": "dev-master",
        "sirprize/scrubble": "dev-master",
        "sirprize/paginate": "dev-master"
    }
}